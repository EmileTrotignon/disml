type snowflake <ocaml from="Snowflake" t="t"> = abstract
type user <ocaml from="User" t="t"> = abstract

type partial_member = {
    ?nick: string option;
    roles: snowflake list;
    joined_at: string;
    deaf: bool;
    mute: bool;
}

type member = {
    inherit partial_member;
    user: user;
}

type member_update = {
    guild_id <json name="id">: snowflake;
    member: member;
}

type t = {
    inherit member;
    guild_id: snowflake;
}